  "description": "JavaScript / TypeScript wrapper for Fortnite-API.com",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx tsc",
    "lint": "eslint src/**/*.ts",
